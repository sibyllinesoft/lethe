#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nathan/Projects/lethe/ctx-run/node_modules/which/bin/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/which/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules:/home/nathan/Projects/lethe/node_modules:/home/nathan/Projects/node_modules:/home/nathan/node_modules:/home/node_modules:/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nathan/Projects/lethe/ctx-run/node_modules/which/bin/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/which/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules:/home/nathan/Projects/lethe/node_modules:/home/nathan/Projects/node_modules:/home/nathan/node_modules:/home/node_modules:/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../which/bin/which.js" "$@"
else
  exec node  "$basedir/../which/bin/which.js" "$@"
fi
