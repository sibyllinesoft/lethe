#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/lerna@8.2.3_encoding@0.1.13/node_modules/lerna/dist/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/lerna@8.2.3_encoding@0.1.13/node_modules/lerna/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/lerna@8.2.3_encoding@0.1.13/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/lerna@8.2.3_encoding@0.1.13/node_modules/lerna/dist/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/lerna@8.2.3_encoding@0.1.13/node_modules/lerna/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/lerna@8.2.3_encoding@0.1.13/node_modules:/home/nathan/Projects/lethe/ctx-run/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lerna/dist/cli.js" "$@"
else
  exec node  "$basedir/../lerna/dist/cli.js" "$@"
fi
